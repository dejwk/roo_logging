{
    "name": "roo_logging",
    "description": "Arduino-compliant ligtweight logging library, targeted at ESP32 and related boards. Port of Google's 'glog' library.",
    "keywords": [
        "logging"
    ],
    "authors": [
        {
            "name": "Dejwk",
            "email": "dejwk.oss@gmail.com",
            "maintainer": true
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dejwk/roo_logging"
    },
    "version": "1.5.1",
    "homepage": "https://github.com/dejwk/roo_logging",
    "frameworks": [
        "*"
    ],
    "build": {
        "srcDir": "src",
        "srcFilter": "+<*> -<examples> -<benchmarks> -<doc> -<test>"
    },
    "examples": [
        "examples/**/*.ino"
    ],
    "dependencies": {
        "dejwk/roo_backport": ">=1.2.0",
        "dejwk/roo_flags": ">=1.2.0",
        "dejwk/roo_time": ">=1.4.0",
        "dejwk/roo_threads": ">=1.2.0"
    }
}